/*Popup styles*/
.overlay_bg{
	background: rgba(54,57,87, 0.7);
	z-index: -200;
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;

}
.overlay{
	visibility: hidden;
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	/*background: rgba(54,57,87, 0.7);*/
	z-index: 200;
}
.popup{
	overflow: auto;
	/*height: 80%;*/

	position: absolute;
	top: 70px;
	left: 20px;
	right: 20px;
	background: #fff;

	/*max-width: 600px;
	position: relative;
	margin: auto;
	top: 70px;*/
}

.popup__close{
	position: absolute;
	right: 15px;
	top: 15px;
	width: 25px;
	height: 25px;
	cursor: pointer;
	background:  url('../images/close.png') no-repeat 50% 50%;
	background-size: contain;
}
.popup__content{
	padding: 24px;
	color: #24204f;
}

.popup .btn{
	display: block;
	margin-bottom: 10px;
	text-align: center;
	height: 40px;
	line-height: 40px;
	background: #24204f;
	color: #fff;
	text-decoration: none;
}
.popup h2{
	font-size: 24px;
	color: #24204f;
	text-transform: uppercase;
	font-weight: normal;
	line-height: 1.2;
	text-align: center;
	margin-bottom: 20px;
}
.popup h3{
	font-size: 18px;
	font-weight: normal;
	text-transform: uppercase;
	text-align: center;
	line-height: 1.2;
	color: #24204f;
	margin-bottom: 17px;
}
.popup p{
	font-size: 12px;
	text-transform: uppercase;
	line-height: 20px;
	margin: 15px 0;
	text-align: center;
}
.popup p a{
	color: #24204f;
}
.popup ul{
	list-style: none;
	padding: 0;
	margin: 0;
	border-bottom: 1px solid #e5e5e5;
}
.popup li{
	border-top: 1px solid #e5e5e5;
}
.popup li a{
	display: block;
	padding: 6px 0 6px 10px;
	color: #24204f;
}

