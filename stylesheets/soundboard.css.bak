.file.file_inner span:after, .checkbox input + span a:after { position: absolute; bottom: 4px; left: 0; right: 0; height: 1px; background: rgba(255, 255, 255, 0.5); }

body { font-family: "FuturaW01Light"; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale; text-transform: uppercase; font-size: 13px; line-height: 20px; color: #fff; }

