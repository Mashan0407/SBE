.title { position: relative; display: inline-block; margin-bottom: 25px; margin-top: 85px; font-size: 40px; text-transform: uppercase; font-family: "NeutrafaceCondensed-Titling"; font-weight: normal; font-style: normal; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale; }
.is-desktop .main:before { background: none; }
.main > section { position: relative; }

